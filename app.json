{
  "expo": {
    "name": "MindCare",
    "slug": "mindcare",
    "version": "1.0.0",
    "scheme": "mindcare",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "android": {
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.app.mindcare"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Este aplicativo precisa acessar a c√¢mera.",
        "NSMicrophoneUsageDescription": "Este aplicativo precisa acessar o microfone."
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "newArchEnabled": true,
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1839274b-024a-4153-8599-964ebfdd0f37"
      }
    }
  }
}
